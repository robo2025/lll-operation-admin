{
  "entry": "src/index.js",
  "extraBabelPlugins": [
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-remove-console"
      ]
    }
  },
  "html": {
    "template": "./src/index.ejs"
  },
  "publicPath": "/",
  "ignoreMomentLocale": true,
  "theme": "./src/theme.js",
  "hash": true
}
